# TVETYDIG AFFILIERING TILL UPPHOVSPERSONPREFIX swpa_m: <http://swepub.kb.se/SwePubAnalysis/model#>PREFIX mods_m: <http://swepub.kb.se/mods/model#> PREFIX xlink: <http://www.w3.org/1999/xlink#> SELECT DISTINCT?_name?_publicationYear?_affiliation_claimsWHERE{{SELECT DISTINCT?_name?_publicationYear sql:group_concat( ?_recordContentSourceValue1, ', ') as ?_affiliation_claimsWHERE{ {SELECT DISTINCT ?_name ?_publicationYear ?_recordContentSourceValue1 WHERE{?AmbiguityCase a swpa_m:AmbiguityCase .?AmbiguityCase swpa_m:localID1 ?_localID1 .?AmbiguityCase swpa_m:violatingData ?_name .?AmbiguityCase swpa_m:publicationYear ?_publicationYear .?AmbiguityCase swpa_m:ambiguityType ?Class .?AmbiguityCase swpa_m:ambiguityType swpa_m:AffiliationFractionConflict .FILTER(?Class = swpa_m:AffiliationFractionConflict) ?Mods1 mods_m:identifierValue ?_localID1 .?Mods1 mods_m:recordContentSourceValue ?_recordContentSourceValue1 .}ORDER BY ?_recordContentSourceValue1 } }GROUP BY ?_name ?_publicationYear ORDER BY ?_name ?_publicationYear }}